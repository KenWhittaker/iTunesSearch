platform :ios, '10.0'

target 'iTunesSearch' do
  use_frameworks!

  # Pods for iTunesSearch
  pod 'SwiftyJSON'
  pod 'Kingfisher', '~> 3.0'

  target 'iTunesSearchTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
  target 'iTunesDataTests' do
      inherit! :search_paths
      # Pods for testing
      pod 'SwiftyJSON'
  end

  target 'iTunesSearchUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
